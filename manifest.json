{
"update_url": "https://clients2.google.com/service/update2/crx", 
    "manifest_version": 2,
    "name": "Posineg",
    "version": "0.0.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["script.js"],
            "css": ["highlight.css"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "description": "Select the text and receive a message whether the selected text's connotation is positive or negative",
    "browser_action": {
        "default_icon": "images/positive-negative.png",
        "default_popup": "popup.html",
        "default_title": "Display sentiment analysis of text"

    }
    
    
}